package org.str;

public class Sample {
	public static void main(String[] args) {
		 String string1 = "Automation Testing";
	        String string2 = "Information";
	        String string3 = "Tamilnadu";
	        String string4 = "j a v a p r o g r a m";
	        String string5 = "987654321";

	        int length1 = string1.length();
	        int length2 = string2.length();
	        int length3 = string3.length();
	        int length4 = string4.length();
	        int length5 = string5.length();

	        System.out.println("Length of String 1: " + length1);
	        System.out.println("Length of String 2: " + length2);
	        System.out.println("Length of String 3: " + length3);
	        System.out.println("Length of String 4: " + length4);
	        System.out.println("Length of String 5: " + length5);
	    }
	
		
		
		
	}


