package org.str;

public class Sample {
	public static void main(String[] args) {
		
		    String string1 = "SoftwareTestEngineer";
	        String string2 = "SeleniumAutomationtool";
	        String string3 = "Goodmorning";
	        String string4 = "j a v a p r o g r a m";
	        String string5 = "9876543210";

	        // Find the character 'e' in String 1
	        char index1 = string1.charAt('e');
	        System.out.println("char at 7 ind: " + index1);

	      
	    }
	}