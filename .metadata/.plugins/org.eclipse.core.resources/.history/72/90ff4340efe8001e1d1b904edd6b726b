package org.str;

public class Sample {
	public static void main(String[] args) {
		  String string1 = "SoftwareTestEngineer";
	        String string2 = "SeleniumAutomationtool";
	        String string3 = "Goodmorning";
	        String string4 = "j a v a p r o g r a m";
	        String string5 = "9876543210";

	        // Find the character 'e' in String 1
	        char a1 = string1.charAt('e');
	        System.out.println("char at 7 ind: " + a1);

	        // Find the character 'o' in String 2
	        char a2 = string2.charAt('m');
	        System.out.println("chat at 11 ind: " + a2);

	        // Find the character 'o' in String 3
	        char a3 = string3.charAt('t');
	        System.out.println("char at 1 ind: " + a3);

	        // Find the character 'p' in String 4
	        char a4 = string4.charAt(' ');
            System.out.println("char at 9 ind: " + a4);

            //Find the character '7' in String 5
            char a5 = string5.charAt('8');
	        System.out.println("char at 2 ind: " + a5);
	    }
	}