package org.str;

public class Sample {
	public static void main(String[] args) {
		
		    String s1 = "SoftwareTestEngineer";
	        String s2 = "SeleniumAutomationtool";
	        String s3 = "Goodmorning";
	        String s4 = "j a v a p r o g r a m";
	        String s5 = "9876543210";

	        // Find the character 'e' in String 1
	        char index1 = s1.charAt(7);
	        System.out.println("char at 7 ind: " + index1);

	        // Find the character 'o' in String 2
	        char index2 = s2.charAt(11);
	        System.out.println("chat at 11 ind: " + index2);

	        // Find the character 'o' in String 3
	        char index3 = s3.charAt(1);
	        System.out.println("char at 1 ind: " + index3);

	        // Find the character 'p' in String 4
	        char index4 = s4.charAt(9);
            System.out.println("char at 9 ind: " + index4);

            //Find the character '7' in String 5
            char index5 = s5.charAt(2);
	        System.out.println("char at 2 ind: " + index5);
	    }
	}